#: import NoTransition kivy.uix.screenmanager.NoTransition
#: import toast kivymd.toast.toast


WindowManager:
	id: scr_mgr
	#transition: NoTransition()
	Startup:
	Login:
	Signup:
	Home:
	ReadWrite:
	Reviews:
	Search:
	Account:
	YourReviews:
	MovieSuggestion:
	Genre:


<Startup>:
	markup: True
	name: "startup"
	

	MDCard:
		pos_hint: {'center_x':0.5,'center_y':0.5}
		padding: 25
		spacing: 25
		size_hint_x: None
		size_hint_y: None
		size: 420,580
		elevation: 30
		orientation: 'vertical'

		Image:
			source: app.img
			allow_stretch: True
			keep_ratio: 0
			mipmap: True
			size_hint: None,None
			size: 240,240
			pos_hint: {'center_x': 0.5,'center_y': 0.5}

		Widget:
			size_hint_y: None
			height: 100

		MDFillRoundFlatButton:
			text: '[b]        Login        [/b]'
			font_size: 22
			background_normal: ''
			md_bg_color: 32/225, 118/255, 247/255, 1
			pos_hint: {'center_x': 0.5}
			on_release: 
				app.root.current = "login"
				root.manager.transition.direction = "left"

		#MDLabel:
			#markup: True
			#text: "[sup]_________[/sup]   or   [sup]_________[/sup]"
			#halign: 'center'
			#height: self.texture_size[1]
			#padding_y: 15


		MDFillRoundFlatButton:
			text: '[b]      Sign up      [/b]'
			font_size: 22	
			background_normal: ''
			md_bg_color: 12/225, 194/255, 24/255, 1	
			pos_hint: {'center_x': 0.5}
			on_release: 
				app.root.current = "signup"
				root.manager.transition.direction = "left"

		Widget:
			size_hint_y: None
			height: 0


<Login>:
	markup: True
	name: "login"

	MDCard:
		pos_hint: {'center_x':0.5,'center_y':0.5}
		padding: 25
		spacing: 25
		size_hint_x: None
		size_hint_y: None
		size: 400,500
		elevation: 15
		orientation: 'vertical'

		MDLabel:
			markup: True
			id: login
			text: '[color=#0080ff][b]Login[/b][/color]'
			font_style: "H3"
			font_size: 35
			halign: 'center'
			size_hint_y: None
			height: self.texture_size[1]
			padding_y: 15

		MDTextField:
			id: loguser
			hint_text: 'Username'
			icon_right: 'account'
			font_size: 20			
			pos_hint: {'center_x':0.5}

		BoxLayout:
			orientation: "vertical"


			MDTextField:
				id: logpassword
				hint_text: 'Password'
				icon_right: 'eye-off'
				font_size: 20
				pos_hint: {'center_x':0.5}
				password: True	  	

			MDFlatButton:
				text: "Forgot password?"
				halign: "left"
				on_release:
					app.forgot_passwd(loguser.text, logpassword.text) if app.is_internet_available() else app.show_alert_dialog(app.no_net)

		Widget:
			size_hint_y: None
			height: 30

		MDFlatButton:
			text: "Don't have an account?"
			halign: "left"
			on_release: 
				app.root.current = "signup"
				loguser.text = ""
				logpassword.text = ""
				root.manager.transition.direction = "left"		

		MDRaisedButton:
			text: '[b]LOGIN[/b]'
			font_size: 14
			size_hint_x: 1
			background_normal: ''
			md_bg_color: 0/255, 100/255, 255/255, 1	
			on_press:
				root.manager.transition = NoTransition()
			on_release: 
				app.logger() if app.is_internet_available() else app.show_alert_dialog(app.no_net)



<Signup>:
	id: signup
	name: "signup"
	markup: True

	MDCard:
		orientation:"vertical"
		size_hint_x: None
		size_hint_y: None
		size:400,500
		pos_hint:{"center_x":.5,"center_y":.5}
		elevation:15
		padding:25
		spacing:25
		
		MDLabel:
			markup: True
			id: signup
			text: '[color=#7231eb][b]Sign Up[/b][/color]'
			font_style: "H3"
			font_size: 35
			halign: 'center'
			size_hint_y: None
			height: self.texture_size[1]
			padding_y: 15
		
		MDTextField:
			id: signupuser
			hint_text:"Username"
			icon_right:"account"
			pos_hint:{"center_x":.5,"center_y":0.65}
			font_size:20

		MDTextField:
			id: signemail
			hint_text:"Email id"
			icon_right:"email"
			pos_hint:{"center_x":.5,"center_y":0.55}
			font_size:20
    
		MDTextField:
			id: signpassword
			hint_text:"Password"
			icon_right:"lock"
			pos_hint:{"center_x":.5,"center_y":0.45}
			font_size:20

		MDFlatButton:
			text: "Already have an account?"
			halign: "left"
			on_release: 
				app.root.current = "login"
				signupuser.text = ""
				signemail.text = ""
				signpassword.text = ""
				root.manager.transition.direction = "left"	
    
		MDRaisedButton:
			text: '[b]SIGNUP[/b]'
			font_size: 14
			size_hint_x: 1
			background_normal: ''
			md_bg_color: 72/255, 7/255, 247/255, 1
			on_press:
				root.manager.transition = NoTransition()
			on_release: 
				app.signer() if app.is_internet_available() else app.show_alert_dialog(app.no_net)

